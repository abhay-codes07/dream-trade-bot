{
  "manifest_version": 3,
  "name": "DreamTrade Auto-Executor",
  "version": "1.1",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["*://*.tradingview.com/*"],
    "js": ["content.js"],
    "run_at": "document_end"
  }]
}